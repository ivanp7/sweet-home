#!/bin/sh

GEOMETRY="${SCRATCHPAD0_GEOMETRY:-}"
WIDTH_RATIO="${SCRATCHPAD0_WIDTH_RATIO:-}"
HEIGHT_RATIO="${SCRATCHPAD0_HEIGHT_RATIO:-}"
WIDTH="${SCRATCHPAD0_WIDTH:-}"
HEIGHT="${SCRATCHPAD0_HEIGHT:-}"
X="${SCRATCHPAD0_X:-}"
Y="${SCRATCHPAD0_Y:-}"

. ../rule_geometry
echo "state=floating sticky=on locked=on rectangle=$GEOMETRY"

