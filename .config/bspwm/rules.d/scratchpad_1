#!/bin/sh

GEOMETRY="${SCRATCHPAD1_GEOMETRY:-}"
WIDTH_RATIO="${SCRATCHPAD1_WIDTH_RATIO:-}"
HEIGHT_RATIO="${SCRATCHPAD1_HEIGHT_RATIO:-}"
WIDTH="${SCRATCHPAD1_WIDTH:-}"
HEIGHT="${SCRATCHPAD1_HEIGHT:-}"
X="${SCRATCHPAD1_X:-}"
Y="${SCRATCHPAD1_Y:-}"

. ../rule_geometry
echo "state=floating sticky=on locked=on rectangle=$GEOMETRY"

