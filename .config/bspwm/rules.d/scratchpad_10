#!/bin/sh

GEOMETRY="${SCRATCHPAD10_GEOMETRY:-}"
WIDTH_RATIO="${SCRATCHPAD10_WIDTH_RATIO:-}"
HEIGHT_RATIO="${SCRATCHPAD10_HEIGHT_RATIO:-}"
WIDTH="${SCRATCHPAD10_WIDTH:-}"
HEIGHT="${SCRATCHPAD10_HEIGHT:-}"
X="${SCRATCHPAD10_X:-}"
Y="${SCRATCHPAD10_Y:-}"

. ../rule_geometry
echo "state=floating sticky=on locked=on rectangle=$GEOMETRY"

