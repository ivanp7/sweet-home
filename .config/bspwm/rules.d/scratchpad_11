#!/bin/sh

GEOMETRY="${SCRATCHPAD11_GEOMETRY:-}"
WIDTH_RATIO="${SCRATCHPAD11_WIDTH_RATIO:-}"
HEIGHT_RATIO="${SCRATCHPAD11_HEIGHT_RATIO:-}"
WIDTH="${SCRATCHPAD11_WIDTH:-}"
HEIGHT="${SCRATCHPAD11_HEIGHT:-}"
X="${SCRATCHPAD11_X:-}"
Y="${SCRATCHPAD11_Y:-}"

. ../rule_geometry
echo "state=floating sticky=on locked=on rectangle=$GEOMETRY"

