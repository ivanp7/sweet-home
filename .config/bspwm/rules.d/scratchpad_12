#!/bin/sh

GEOMETRY="${SCRATCHPAD12_GEOMETRY:-}"
WIDTH_RATIO="${SCRATCHPAD12_WIDTH_RATIO:-}"
HEIGHT_RATIO="${SCRATCHPAD12_HEIGHT_RATIO:-}"
WIDTH="${SCRATCHPAD12_WIDTH:-}"
HEIGHT="${SCRATCHPAD12_HEIGHT:-}"
X="${SCRATCHPAD12_X:-}"
Y="${SCRATCHPAD12_Y:-}"

. ../rule_geometry
echo "state=floating sticky=on locked=on rectangle=$GEOMETRY"

