#!/bin/sh

GEOMETRY="${SCRATCHPAD2_GEOMETRY:-}"
WIDTH_RATIO="${SCRATCHPAD2_WIDTH_RATIO:-}"
HEIGHT_RATIO="${SCRATCHPAD2_HEIGHT_RATIO:-}"
WIDTH="${SCRATCHPAD2_WIDTH:-}"
HEIGHT="${SCRATCHPAD2_HEIGHT:-}"
X="${SCRATCHPAD2_X:-}"
Y="${SCRATCHPAD2_Y:-}"

. ../rule_geometry
echo "state=floating sticky=on locked=on rectangle=$GEOMETRY"

