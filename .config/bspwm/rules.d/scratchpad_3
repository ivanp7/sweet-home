#!/bin/sh

GEOMETRY="${SCRATCHPAD3_GEOMETRY:-}"
WIDTH_RATIO="${SCRATCHPAD3_WIDTH_RATIO:-}"
HEIGHT_RATIO="${SCRATCHPAD3_HEIGHT_RATIO:-}"
WIDTH="${SCRATCHPAD3_WIDTH:-}"
HEIGHT="${SCRATCHPAD3_HEIGHT:-}"
X="${SCRATCHPAD3_X:-}"
Y="${SCRATCHPAD3_Y:-}"

. ../rule_geometry
echo "state=floating sticky=on locked=on rectangle=$GEOMETRY"

