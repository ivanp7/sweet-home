#!/bin/sh

GEOMETRY="${SCRATCHPAD4_GEOMETRY:-}"
WIDTH_RATIO="${SCRATCHPAD4_WIDTH_RATIO:-}"
HEIGHT_RATIO="${SCRATCHPAD4_HEIGHT_RATIO:-}"
WIDTH="${SCRATCHPAD4_WIDTH:-}"
HEIGHT="${SCRATCHPAD4_HEIGHT:-}"
X="${SCRATCHPAD4_X:-}"
Y="${SCRATCHPAD4_Y:-}"

. ../rule_geometry
echo "state=floating sticky=on locked=on rectangle=$GEOMETRY"

