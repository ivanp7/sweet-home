#!/bin/sh

GEOMETRY="${SCRATCHPAD5_GEOMETRY:-}"
WIDTH_RATIO="${SCRATCHPAD5_WIDTH_RATIO:-}"
HEIGHT_RATIO="${SCRATCHPAD5_HEIGHT_RATIO:-}"
WIDTH="${SCRATCHPAD5_WIDTH:-}"
HEIGHT="${SCRATCHPAD5_HEIGHT:-}"
X="${SCRATCHPAD5_X:-}"
Y="${SCRATCHPAD5_Y:-}"

. ../rule_geometry
echo "state=floating sticky=on locked=on rectangle=$GEOMETRY"

