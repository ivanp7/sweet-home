#!/bin/sh

GEOMETRY="${SCRATCHPAD6_GEOMETRY:-}"
WIDTH_RATIO="${SCRATCHPAD6_WIDTH_RATIO:-}"
HEIGHT_RATIO="${SCRATCHPAD6_HEIGHT_RATIO:-}"
WIDTH="${SCRATCHPAD6_WIDTH:-}"
HEIGHT="${SCRATCHPAD6_HEIGHT:-}"
X="${SCRATCHPAD6_X:-}"
Y="${SCRATCHPAD6_Y:-}"

. ../rule_geometry
echo "state=floating sticky=on locked=on rectangle=$GEOMETRY"

