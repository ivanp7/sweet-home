#!/bin/sh

GEOMETRY="${SCRATCHPAD7_GEOMETRY:-}"
WIDTH_RATIO="${SCRATCHPAD7_WIDTH_RATIO:-}"
HEIGHT_RATIO="${SCRATCHPAD7_HEIGHT_RATIO:-}"
WIDTH="${SCRATCHPAD7_WIDTH:-}"
HEIGHT="${SCRATCHPAD7_HEIGHT:-}"
X="${SCRATCHPAD7_X:-}"
Y="${SCRATCHPAD7_Y:-}"

. ../rule_geometry
echo "state=floating sticky=on locked=on rectangle=$GEOMETRY"

