#!/bin/sh

GEOMETRY="${SCRATCHPAD8_GEOMETRY:-}"
WIDTH_RATIO="${SCRATCHPAD8_WIDTH_RATIO:-}"
HEIGHT_RATIO="${SCRATCHPAD8_HEIGHT_RATIO:-}"
WIDTH="${SCRATCHPAD8_WIDTH:-}"
HEIGHT="${SCRATCHPAD8_HEIGHT:-}"
X="${SCRATCHPAD8_X:-}"
Y="${SCRATCHPAD8_Y:-}"

. ../rule_geometry
echo "state=floating sticky=on locked=on rectangle=$GEOMETRY"

