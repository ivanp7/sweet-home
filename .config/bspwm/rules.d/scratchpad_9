#!/bin/sh

GEOMETRY="${SCRATCHPAD9_GEOMETRY:-}"
WIDTH_RATIO="${SCRATCHPAD9_WIDTH_RATIO:-}"
HEIGHT_RATIO="${SCRATCHPAD9_HEIGHT_RATIO:-}"
WIDTH="${SCRATCHPAD9_WIDTH:-}"
HEIGHT="${SCRATCHPAD9_HEIGHT:-}"
X="${SCRATCHPAD9_X:-}"
Y="${SCRATCHPAD9_Y:-}"

. ../rule_geometry
echo "state=floating sticky=on locked=on rectangle=$GEOMETRY"

